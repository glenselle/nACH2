{
  "name": "nach2",
  "description": "nACH2 is a highly customizable Node.js module exposing a high & low-level API for generating ACH files for use within the ACH network",
  "version": "1.0.0-next.0",
  "keywords": [
    "ACH",
    "file",
    "Automated Clearing House"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/glenselle/nACH2.git"
  },
  "engines": {
    "node": ">=14.x.x"
  },
  "scripts": {
    "test": "jest --coverage"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "coverageDirectory": "coverage",
    "testRegex": "((\\.|/*.)(test))\\.js?$"
  },
  "dependencies": {
    "async": "^2.0.1",
    "luxon": "^3.7.1"
  },
  "devDependencies": {
    "jest": "^30.0.5"
  }
}
